plugin.bootstrap_package.settings.less.container-lg = 100%
plugin.bootstrap_package.settings.less.container-md = 100%
plugin.bootstrap_package.settings.less.container-sm = 100%
plugin.bootstrap_package.settings.less.container-tablet = 100%

page.favicon.file = EXT:unal_skin_default/Resources/Public/images/favicon.ico

page.preloader.enable = 0
page.preloader.logo.file = EXT:unal_skin_default/Resources/Public/images/escudoUnal.svg
page.preloader.logo.height = 200
page.preloader.logo.width = 600

