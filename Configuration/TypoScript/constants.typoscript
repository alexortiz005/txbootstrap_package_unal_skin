plugin.bootstrap_package.settings.less.body-bg = #ddd
plugin.bootstrap_package.settings.less.font-size-base = 14px
plugin.bootstrap_package.settings.less.font-family-base = "Ancizar sans", Tahoma, Geneva, sans-serif;
plugin.bootstrap_package.settings.less.brand-primary = #4b4b4b;
plugin.bootstrap_package.settings.less.line-height-base = 1.42857143
plugin.bootstrap_package.settings.less.link-color = #94b43b;
plugin.bootstrap_package.settings.less.container-desktop = 100%
